# Protocol Library Environment Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_USE_MOCK_API=false

# Application Settings
REACT_APP_APP_NAME=Protocol Library
REACT_APP_VERSION=1.0.0

# Polling Configuration
REACT_APP_POLLING_INTERVAL=5000
REACT_APP_MAX_POLLING_ATTEMPTS=100

# Feature Flags
REACT_APP_ENABLE_FULLSCREEN=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_THEMES=false

# Development Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
